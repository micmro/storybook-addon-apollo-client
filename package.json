{
  "author": {
    "name": "Aaron Reisman",
    "email": "aaron@lifeiscontent.net",
    "url": "https://lifeiscontent.net/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lifeiscontent/storybook-addon-apollo-client.git"
  },
  "devDependencies": {
    "@apollo/client": "3.x",
    "@rollup/plugin-multi-entry": "^4.0.0",
    "@types/jest": "25.1.4",
    "graphql": "15.3.0",
    "husky": "4.2.3",
    "react": "16.x",
    "react-dom": "16.13.1",
    "tsdx": "0.13.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@storybook/addons": "6.x",
    "@storybook/components": "6.x"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./register": {
      "import": "./dist/register.esm.js",
      "require": "./dist/register.js"
    }
  },
  "name": "storybook-addon-apollo-client",
  "peerDependencies": {
    "@apollo/client": "3.x",
    "react": "16.x"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build --entry src/index.tsx --entry src/register.tsx && npm run post-build",
    "post-build": "node scripts/post-tsdx.js",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build"
  },
  "typings": "dist/index.d.ts",
  "version": "3.0.0"
}
